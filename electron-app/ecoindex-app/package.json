        "@rauschma/stringio": "^1.4.0",
        "@tailwindcss/forms": "^0.5.7",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.1",
        "electron-log": "^5.1.7",
        "electron-squirrel-startup": "^1.0.1",
        "fix-path": "^4.0.0",
        "lucide-react": "^0.412.0",
        "postcss": "^8.4.38",
        "postcss-loader": "^8.1.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-icons": "^5.2.1",
