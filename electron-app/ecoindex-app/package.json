{
    "name": "ecoindex-lighthouse",
    "displayName": "EcoindexLighthouse",
    "productName": "EcoindexLighthouse",
    "private": true,
    "version": "1.2.1-rc.14",
    "description": "An application to mesure the ecological impact of a website with LightHouse and Ecoindex.",
    "homepage": "https://github.com/cnumr/lighthouse-plugin-ecoindex#readme",
    "bugs": "https://github.com/cnumr/lighthouse-plugin-ecoindex/issues",
    "publisher": "Association Green IT",
    "license": "MIT",
    "author": {
        "name": "Renaud Héluin",
        "email": "renaud.heluin@novagaia.fr",
        "url": "https://novagaia.fr"
    },
    "contributors": [
        {
            "name": "Renaud Héluin",
            "email": "rheluin@novagaia.fr"
        }
    ],
    "keywords": [
        "lighthouse",
        "plugin",
        "ecoindex",
        "GreenIT.fr",
        "electron"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/cnumr/lighthouse-plugin-ecoindex.git"
    },
    "main": ".webpack/main",
    "scripts": {
        "start": "env-cmd electron-forge start",
        "package": "env-cmd electron-forge package",
        "clean:out": "rm -rf ./out/* --y",
        "make": "env-cmd electron-forge make",
        "make:ci": "electron-forge make",
        "make:darwin:arm64": "env-cmd electron-forge make --platform=darwin --arch=arm64",
        "make:darwin:x64": "env-cmd electron-forge make --platform=darwin --arch=x64",
        "make:win32:x64": "PATH=$(pwd)/wine_proxy:$PATH env-cmd electron-forge make --platform=win32 --arch=x64",
        "make:win32:arm64": "PATH=$(pwd)/wine_proxy:$PATH env-cmd electron-forge make --platform=win32 --arch=arm64",
        "publish": "env-cmd electron-forge publish",
        "publish:ci": "electron-forge publish",
        "lint": "eslint --ext .ts,.tsx .",
        "release:local": "env-cmd release-it --preRelease=rc --config .local.release-it.json",
        "release:local:patch": "env-cmd release-it --config .local.release-it.json",
        "release:local:minor": "env-cmd release-it minor --config .local.release-it.json",
        "release:local:major": "env-cmd release-it major --config .local.release-it.json",
        "release:ci": "release-it"
    },
    "devDependencies": {
        "@electron-forge/cli": "^7.4.0",
        "@electron-forge/maker-deb": "^7.4.0",
        "@electron-forge/maker-dmg": "^7.4.0",
